	file = {Akbari_Fernando_2015_Modeling and optimization of PHEV charging queues.pdf:D\:\\ZoteroData\\storage\\KD9HVYLP\\Akbari_Fernando_2015_Modeling and optimization of PHEV charging queues.pdf:application/pdf},
}

@article{li_ev_2022,
	title = {{EV} {Charging} {Strategy} {Considering} {Transformer} {Lifetime} via {Evolutionary} {Curriculum} {Learning}-{Based} {Multiagent} {Deep} {Reinforcement} {Learning}},
	volume = {13},
	issn = {1949-3053, 1949-3061},
	url = {https://ieeexplore.ieee.org/document/9756505/},
	doi = {10.1109/TSG.2022.3167021},
	abstract = {��近年来，随着电动汽车（EV）的普及，配电变压器的寿命损失（LOL）加快。本文提出了一种基于进化课程学习（ECL）的多智能体深度强化学习（MADRL）方法，以优化变压器LOL，同时考虑不同电动汽车车主的各种充电需求。具体来说，为多辆电动汽车充电的问题被看作是一个马尔可夫博弈。通过将每个EV控制器建模为具有特定目标的代理，通过所提出的MADRL算法来解决该问题。在集中训练阶段，采用了一种新的集中ECL机制来增强多个EV的协调。它使所提出的方法能够解决大规模电动汽车充电的管理问题。培训程序完成后，以分散的方式部署拟议的方法。在这里，所有代理仅基于本地信息做出决策。分散的执行方式有助于保护电动汽车所有者的隐私，降低相关的通信成本，并避免单点故障。利用真实世界数据的对比测试表明，所提出的方法可以实现大量电动汽车的协调充电，同时满足不同电动汽车车主的各种充电需求。An accelerated loss of life (LOL) of distribution transformers has been observed in recent years owing to the increasing penetration of electric vehicles (EVs). This paper proposes an evolutionary curriculum learning (ECL)-based multi-agent deep reinforcement learning (MADRL) approach for optimizing transformer LOL while considering various charging demands of different EV owners. Specifically, the problem of charging multiple EVs is cast as a Markov game. It is solved by the proposed MADRL algorithm by modeling each EV controller as an agent with a specific objective. During the centralized training stage, a novel centralized ECL mechanism is adopted to enhance the coordination of multiple EVs. It enables the proposed approach to address the management of large-scale EV charging. When the training procedure is completed, the proposed approach is deployed in a decentralized manner. Herein, all the agents make decisions based solely on local information. The decentralized manner of execution helps preserve the privacy of EV owners, reduce the related communication cost, and avoid single-point failure. Comparative tests utilizing real-world data demonstrate that the proposed approach can achieve coordinated charging of a large number of EVs while satisfying the various charging demands of different EV owners.},
	number = {4},
	urldate = {2023-03-08},
	journal = {IEEE Transactions on Smart Grid},
	author = {Li, Sichen and Hu, Weihao and Cao, Di and Zhang, Zhenyuan and Huang, Qi and Chen, Zhe and Blaabjerg, Frede},
	month = jul,
	year = {2022},
	keywords = {/unread},
	pages = {2774--2787},
	file = {Li et al_2022_EV Charging Strategy Considering Transformer Lifetime via Evolutionary.pdf:D\:\\ZoteroData\\storage\\7TS3CEVQ\\Li et al_2022_EV Charging Strategy Considering Transformer Lifetime via Evolutionary.pdf:application/pdf},
}

@article{lai_pricing_2023,
	title = {Pricing for {Electric} {Vehicle} {Charging} {Stations} {Based} on the {Responsiveness} of {Demand}},
	volume = {14},
	issn = {1949-3053, 1949-3061},
	url = {https://ieeexplore.ieee.org/document/9816010/},
	doi = {10.1109/TSG.2022.3188832},
	abstract = {��电动汽车（EV）在减少运输部门的温室气体排放方面具有巨大潜力。因此，电动汽车在一些国家的普及率稳步增长。然而，电动汽车的扩散会对配电网（PDN）和运输网（TN）造成负面影响。因此，本文提出了电动汽车充电站（EVCS）的动态定价策略，以帮助EVCS获得更高的利润，同时减轻对PDN和TN的潜在负面影响。第二，提出了一种两步方法，以数学形式表示需求对收费价格的响应性。第三，基于准入控制方案和排队模型结合EV用户行为，以进一步调整充电需求。我们进行了模拟，以验证PDN和TN耦合系统中定价策略的有效性，该系统包含约2000辆电动汽车。结果表明，与其他三种情况相比，所提出的定价策略可以帮助EVCS平均获得14.2\%的净充电利润。此外，由于与其他三种情况相比，所提出的定价策略可以产生最小的排队长度波动和适度的利润方差，因此可以确保EVCS的运行稳定性。此外，所提出的定价策略可以通过激励电动汽车改变其站点选择决策来实现空间负荷转移，以避免电网中可能的电力拥堵。Electric vehicles (EVs) have the promising potential to be effective in mitigating greenhouse gas emissions in the transportation sector. Hence, the penetration of EVs in some countries experiences steady growth. However, the proliferation of EVs can cause negative impacts on the power distribution network (PDN) and the transportation network (TN). Thus, in this paper, a dynamic pricing strategy for electric vehicle charging stations (EVCSs) is proposed to facilitate EVCSs to earn higher profits while alleviating the potential negative impacts on both PDN and TN. First, a pricing strategy, considering the competition effect established based on the TN, is formulated to facilitate the EVCS to attract the defined competitive charging demand. Second, a two-step approach is proposed to mathematically formulate the responsiveness of demand towards the charging price. Third, EV user behaviors are incorporated based on both an admission control scheme and a queueing model to further adjust the charging demand. We have conducted simulations to verify the effectiveness of the pricing strategy in a PDN and TN coupled system, which contains approximately 2000 EVs. Results show that the proposed pricing strategy can facilitate the EVCS to gain 14.2\% more net charging profit on average compared with the other three cases. Moreover, the operational stability of the EVCS can be ensured because the proposed pricing strategy can result in the least queueing length volatility with moderate profit variance compared with the other three cases. Furthermore, the proposed pricing strategy can achieve spatial load shifting by incentivizing EVs to alter their station-selection decisions to avoid possible power congestion in the electricity network.},
	number = {1},
	urldate = {2023-03-08},
	journal = {IEEE Transactions on Smart Grid},
	author = {Lai, Shuying and Qiu, Jing and Tao, Yuechuan and Zhao, Junhua},
	month = jan,
	year = {2023},
	keywords = {/unread},
	pages = {530--544},
	file = {Lai et al_2023_Pricing for Electric Vehicle Charging Stations Based on the Responsiveness of.pdf:D\:\\ZoteroData\\storage\\3HXPJK39\\Lai et al_2023_Pricing for Electric Vehicle Charging Stations Based on the Responsiveness of.pdf:application/pdf},
}

@article{said_novel_2020,
	title = {A {Novel} {Electric} {Vehicles} {Charging}/{Discharging} {Management} {Protocol} {Based} on {Queuing} {Model}},
	volume = {5},
	issn = {2379-8904, 2379-8858},
	url = {https://ieeexplore.ieee.org/document/8910498/},
	doi = {10.1109/TIV.2019.2955370},
	abstract = {��预计电动汽车（EV）的普及率将增加智能电网的需求，特别是在各种电动汽车充电需求的情况下。因此，必须对供电站的电动汽车充电过程进行管理，以提高电动汽车满意度，同时保持智能电网的稳定性。在本文中，电动汽车与电网之间的双向电力流；网格到车辆（G2V）和车辆到网格（V2G）。我们从电动汽车的未使用电力中获利，并提出了一种基于电动汽车充电和放电协调的电动汽车负载管理技术。我们提出了一种峰值负载管理模型（PLM），用于根据电力需求以及每个电动汽车需要服务的时间和位置来调度电动汽车的充电或放电服务。此外，我们还提出了电动车辆供电设备（EVSE）选择模型，以引导电动车辆到达供电站。基于排队理论，我们开发了一种处理EVSE充电/放电请求的数学形式。这些模型在考虑城市场景中车辆的移动性和使用时间定价（TOUP）的同时进行评估。最后，进行了大量的matlab仿真，以验证所提出的方法并证明其有效性。High electric vehicles (EVs) penetration is expected to increase smart grid solicitation especially with various EV charging demands. As result, the EV charging process at the supply station has to be managed in the way to promote the EV satisfaction level while preserving smart grid stability. In this article, the bidirectional power flow between EV and grid; Grid-to-Vehicle (G2V) and Vehicle-to-Grid (V2G), is exploited. We make a profit from the unused electric power of EVs and we present an EV load management technique based on EV charging and EV discharging coordination. We propose a peak load management model (PLM) used to schedule EVs for charging or discharging service according to the power demand with the timing and location where each EV need to be served. Also, we propose an Electric Vehicle Supply Equipment (EVSE) selection model to guide EVs to the supply station. We develop a mathematical formalism for handling requests for EV charging/discharging at EVSE based on queuing theory. Those models are evaluated while considering the mobility of vehicles in an urban scenario and time-of-use-pricing (TOUP). Finally, extensive matlab simulations are conducted to validate the proposed approach and demonstrate its effectiveness.},
	number = {1},
	urldate = {2023-03-08},
	journal = {IEEE Transactions on Intelligent Vehicles},
	author = {Said, Dhaou and Mouftah, Hussein T.},
	month = mar,
	year = {2020},
	keywords = {/unread},
	pages = {100--111},
	file = {Said_Mouftah_2020_A Novel Electric Vehicles Charging-Discharging Management Protocol Based on.pdf:D\:\\ZoteroData\\storage\\5NLN4FLB\\Said_Mouftah_2020_A Novel Electric Vehicles Charging-Discharging Management Protocol Based on.pdf:application/pdf},
}

@article{jiang_data-driven_2023,
	title = {Data-{Driven} {Low}-{Rank} {Tensor} {Approximation} for {Fast} {Grid} {Integration} of {Commercial} {EV} {Charging} {Stations} {Considering} {Demand} {Uncertainties}},
	volume = {14},
	issn = {1949-3053, 1949-3061},
	shorttitle = {��考虑需求不确定性的商用电动汽车充电站快速电网集成的数据驱动低阶张量近似},
	url = {https://ieeexplore.ieee.org/document/9830763/},
	doi = {10.1109/TSG.2022.3191530},
	abstract = {��商用快速电动汽车（EV）充电站的高功率需求和充电变化有可能显著影响配电系统的运行。为了评估这种影响，工程实践是在峰值负载下运行潮流研究，得出保守的结果，或实施计算成本高昂的准静态时间序列模拟。为了克服这些缺点，本文提出了一种基于低秩张量近似（LRA）的计算方法，用于快速研究商业电动汽车充电站在需求不确定的情况下对配电网运行的影响。为此，实施了一个随机运输模型，以基于车辆驾驶模式预测电动汽车站的潜在充电曲线，并使用预测的充电需求构建LRA算法的多项式基。提出了一种基于聚类方法的方法，以策略性地选择实验样本以优化系数的确定，并自适应地配置LRA模型的秩和度以提高性能。在现实世界的公用馈线上的测试案例表明，所提出的方法能够潜在地将配电馈线电路的时间序列模拟时间减少99.99\%左右，同时保持高精度。The high power demand and charging variation of commercial fast electric vehicle (EV) charging stations has the potential to significantly impact the operation of electric power distribution systems. To evaluate this impact, the engineering practice is to run power flow studies at peak load, leading to a conservative result, or implement computationally expensive quasi-static time-series simulations. To overcome these drawbacks, this paper proposes a computational approach based on low-rank tensor approximation (LRA) for fast grid impact studies on distribution grid operation caused by commercial EV charging stations with demand uncertainties. To this end, a stochastic transportation model is implemented to predict the potential charging profiles of EV stations based on vehicles’ driving patterns, and the predicted charging demand is used to construct polynomial bases for the LRA algorithm. A methodology based on clustering approaches is proposed to strategically select the experimental samples for optimized determination of the coefficients, and the rank and degree of the LRA model are adaptively configured for improved performance. Test cases on a real-world utility feeder have demonstrated that the proposed approach is able to potentially reduce the time-series simulation time of distribution feeder circuits by around 99.99\% while maintaining a high accuracy.},
	number = {1},
	urldate = {2023-03-08},
	journal = {IEEE Transactions on Smart Grid},
	author = {Jiang, Yazhou and Ortmeyer, Thomas H. and Fan, Miao and Ai, Xiaomeng},
	month = jan,
	year = {2023},
	keywords = {/unread},
	pages = {517--529},
}

@article{zhang_optimal_2019,
	title = {Optimal {Charging} {Scheduling} by {Pricing} for {EV} {Charging} {Station} {With} {Dual} {Charging} {Modes}},
	volume = {20},
	issn = {1524-9050, 1558-0016},
	url = {https://ieeexplore.ieee.org/document/8520744/},
	doi = {10.1109/TITS.2018.2876287},
	abstract = {��随着电动汽车（EV）的日益普及和各种用户偏好，充电站通常提供几种不同的充电模式以满足EV的各种需求。如何有效利用充电容量，最大限度地降低服务掉线率，是充电站面临的一个紧迫而开放的问题。鉴于电动汽车车主对充电模式的价格敏感，我们打算设计一个最优定价方案，以最小化充电站的服务下降率。首先，我们将双模充电站的操作表述为具有多个服务器和异构服务速率的排队网络，并分析充电站的服务丢弃率与电动汽车选择之间的关系。然后，我们制定了一个客户损耗最小化问题，以最小化离开充电站而未充电的电动汽车的数量，并提出了一种最优定价方法，以指导和协调充电站中电动汽车的充电过程。进行了仿真，以评估所提出的充电调度方案的性能，并显示所提出的定价方案的效率。With the increasing penetration of electric vehicles (EVs) and various user preferences, charging stations often provide several different charging modes to satisfy the various requirements of EVs. How to effectively utilize the charging capacity to minimize the service dropping rate is a pressing and open issue for charging stations. Given that EV owners are price-sensitive to the charging modes, we intend to design an optimal pricing scheme to minimize the service dropping rate of the charging station. First, we formulate the operation of a dual-mode charging station as a queuing network with multiple servers and heterogeneous service rates, and analyze the relationship between the service dropping rate of the charging station and the selections of EVs. Then, we formulate a customer attrition minimization problem to minimize the number of EVs that leave the charging station without being charged and propose an optimal pricing approach to guide and coordinate the charging processes of EVs in the charging station. The simulation has been conducted to evaluate the performance of the proposed charging scheduling scheme and show the efficiency of the proposed pricing scheme.},
	number = {9},
	urldate = {2023-03-10},
	journal = {IEEE Transactions on Intelligent Transportation Systems},
	author = {Zhang, Yongmin and You, Pengcheng and Cai, Lin},
	month = sep,
	year = {2019},
	keywords = {/unread},
	pages = {3386--3396},
	file = {Zhang et al_2019_Optimal Charging Scheduling by Pricing for EV Charging Station With Dual.pdf:D\:\\ZoteroData\\storage\\EHLWV38Q\\Zhang et al_2019_Optimal Charging Scheduling by Pricing for EV Charging Station With Dual.pdf:application/pdf;Zhang et al_2019_Optimal Charging Scheduling by Pricing for EV Charging Station With Dual.pdf:D\:\\ZoteroData\\storage\\C7MK3XP8\\Zhang et al_2019_Optimal Charging Scheduling by Pricing for EV Charging Station With Dual.pdf:application/pdf},
}

@article{fang_aggregator-based_2020,
	title = {Aggregator-based demand response mechanism for electric vehicles participating in peak regulation in valley time of receiving-end power grid},
	volume = {3},
	issn = {20965117},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S2096511720301122},
	doi = {10.1016/j.gloei.2020.11.005},
	abstract = {��随着受电比例的增加和当地机组调峰能力的不足，受电端电网难以实现谷时调峰。为了解决这一问题，同时考虑到电动汽车（EV）的大规模充电负载的潜力，本研究提出了一种基于聚合器的需求响应（DR）机制，用于在山谷时间参与峰值调节的电动汽车。在这种基于聚合器的DR机制中，考虑了电网运营的利润和电动汽车所有者的参与意愿。基于电动汽车充电过程和日前机组发电调度的特点，建立了具有DR的滚动机组承诺模型，以实现社会福利的最大化。此外，为了提高优化问题解决过程的效率并实现独立系统运营商（ISO）和聚合器之间的通信，使用聚类算法来提取典型的EV充电模式。最后，通过案例研究验证了基于聚合器的DR机制在节约成本和减少接收端电网峰谷差方面的可行性和益处。With the increase in the power receiving proportion and an insufficient peak regulation capacity of the local units, the receiving-end power grid struggles to achieve peak regulation in valley time. To solve this problem while considering the potential of the large-scale charge load of electric vehicles (EVs), an aggregator-based demand response (DR) mechanism for EVs that are participating in the peak regulation in valley time is proposed in this study. In this aggregator-based DR mechanism, the profits for the power grid’s operation and the participation willingness of the EV owners are considered. Based on the characteristics of the EV charging process and the day-ahead unit generation scheduling, a rolling unit commitment model with the DR is established to maximize the social welfare. In addition, to improve the efficiency of the optimization problem solving process and to achieve communication between the independent system operator (ISO) and the aggregators, the clustering algorithm is utilized to extract typical EV charging patterns. Finally, the feasibility and benefits of the aggregator-based DR mechanism for saving the costs and reducing the peak-valley difference of the receiving-end power grid are verified through case studies.},
	language = {en},
	number = {5},
	urldate = {2023-03-12},
	journal = {Global Energy Interconnection},
	author = {Fang, Chen and Zhao, Xiaojin and Xu, Qin and Feng, Donghan and Wang, Haojing and Zhou, Yun},
	month = oct,
	year = {2020},
	keywords = {/unread},
	pages = {453--463},
	file = {Fang et al_2020_Aggregator-based demand response mechanism for electric vehicles participating.pdf:D\:\\ZoteroData\\storage\\U2DEJMFF\\Fang et al_2020_Aggregator-based demand response mechanism for electric vehicles participating.pdf:application/pdf},
}

