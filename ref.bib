@article{li_constrained_2020,
	title = {Constrained {EV} {Charging} {Scheduling} {Based} on {Safe} {Deep} {Reinforcement} {Learning}},
	volume = {11},
	issn = {1949-3053, 1949-3061},
	url = {https://ieeexplore.ieee.org/document/8910361/},
	doi = {10.1109/TSG.2019.2955437},
	abstract = {��电动汽车（EV）在过去几年中被广泛采用和部署，因为它们是环保的。当集成到智能电网中时，电动汽车可以作为灵活的负载或储能设备运行，以参与需求响应（DR）。通过利用DR中的时变电价，可以通过优化充电/放电时间表来降低充电成本。然而，由于EV的到达和离开时间以及电价存在随机性，因此难以确定最佳充电/放电时间表以确保EV在离开时完全充电。为了解决这个问题，我们将电动汽车充电/放电调度问题表述为约束马尔可夫决策过程（CMDP）。目的是找到一种受限的充电/放电调度策略，以最小化充电成本，并确保电动汽车能够充满电。为了解决CMDP，提出了一种基于安全深度强化学习（SDRL）的无模型方法。所提出的方法不需要关于随机性的任何领域知识。它直接学习用深度神经网络（DNN）生成约束的最优充电/放电计划。与现有的强化学习（RL）或深度RL（DRL）范式不同，所提出的方法不需要手动设计惩罚项或调整惩罚系数。实际电价的数值实验证明了所提出方法的有效性。Electric vehicles (EVs) have been popularly adopted and deployed over the past few years because they are environment-friendly. When integrated into smart grids, EVs can operate as flexible loads or energy storage devices to participate in demand response (DR). By taking advantage of time-varying electricity prices in DR, the charging cost can be reduced by optimizing the charging/discharging schedules. However, since there exists randomness in the arrival and departure time of an EV and the electricity price, it is difficult to determine the optimal charging/discharging schedules to guarantee that the EV is fully charged upon departure. To address this issue, we formulate the EV charging/discharging scheduling problem as a constrained Markov Decision Process (CMDP). The aim is to find a constrained charging/discharging scheduling strategy to minimize the charging cost as well as guarantee the EV can be fully charged. To solve the CMDP, a model-free approach based on safe deep reinforcement learning (SDRL) is proposed. The proposed approach does not require any domain knowledge about the randomness. It directly learns to generate the constrained optimal charging/discharging schedules with a deep neural network (DNN). Unlike existing reinforcement learning (RL) or deep RL (DRL) paradigms, the proposed approach does not need to manually design a penalty term or tune a penalty coefficient. Numerical experiments with real-world electricity prices demonstrate the effectiveness of the proposed approach.},
	number = {3},
	urldate = {2023-03-07},
	journal = {IEEE Transactions on Smart Grid},
	author = {Li, Hepeng and Wan, Zhiqiang and He, Haibo},
	month = may,
	year = {2020},
	keywords = {/unread},
	pages = {2427--2439},
	file = {Li et al_2020_Constrained EV Charging Scheduling Based on Safe Deep Reinforcement Learning.pdf:D\:\\ZoteroData\\storage\\5QQT625I\\Li et al_2020_Constrained EV Charging Scheduling Based on Safe Deep Reinforcement Learning.pdf:application/pdf},
}

