@article{subramanian_two-layer_2019,
	title = {A two-layer model for dynamic pricing of electricity and optimal charging of electric vehicles under price spikes},
	volume = {167},
	issn = {03605442},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0360544218321686},
	doi = {10.1016/j.energy.2018.10.171},
	abstract = {��在各大洲开展的电力网络试点项目通过提高需求响应，确立了动态定价的优势。然而，需求响应增长的一个关键障碍是缺乏先进的计量基础设施，这阻碍了动态定价的采用。我们相信，电动汽车（EV）的增长将部分解决这一障碍，因为智能和联网的EV停车场将成为需求响应的提供者。我们开发了一个两层优化模型，该模型同时确定了系统运营商的动态定价策略和电动汽车停车场的需求响应策略。该模型最大限度地降低了消费者的成本，同时确保了系统运营商的收入中性状态，并解决了实时价格不确定性。使用5总线PJM网络的变体来演示模型实现。数值结果表明，对于低到中等的价格飙升情况，与统一定价相比，仅电动汽车的需求响应动态定价可以降低1.42\%的日均消费者成本。当价格峰值相对较高时，成本可降低6.5\%。在结束语中讨论了在实际网络中实现我们的模型的计算挑战。Pilot projects in power networks conducted across continents have established the benefits of dynamic pricing by inducing increased demand response. However, a key hurdle in the growth of demand response is the lack of widespread availability of advanced metering infrastructure, which has stymied the adoption of dynamic pricing. We believe that this hurdle will be partially addressed by the growth of electric vehicles (EVs), as smart and connected EV parking lots will be a provider of demand response. We develop a two-layer optimization model that simultaneously determines dynamic pricing policy for the system operator and demand response strategies for the EV parking lots. The model minimizes the cost to consumers, while ensuring the system operator's revenue neutral status and addressing real-time price uncertainties. A variant of the 5-bus PJM network is used to demonstrate model implementation. Numerical results show that for a low to moderate price spike scenario, dynamic pricing with demand response from EVs alone can lower the daily average consumer cost of 1.42\% compared to the cost of flat pricing. A cost reduction of 6.5\% is achieved when price spikes are relatively high. Computational challenges of implementing our model for real networks are discussed in the concluding remarks.},
	language = {en},
	urldate = {2023-03-07},
	journal = {Energy},
	author = {Subramanian, Vignesh and Das, Tapas K.},
	month = jan,
	year = {2019},
	keywords = {/unread},
	pages = {1266--1277},
}

