@article{hung_novel_2022,
	title = {A {Novel} {Data}-{Driven} {Approach} for {Solving} the {Electric} {Vehicle} {Charging} {Station} {Location}-{Routing} {Problem}},
	volume = {23},
	issn = {1524-9050, 1558-0016},
	url = {https://ieeexplore.ieee.org/document/9868264/},
	doi = {10.1109/TITS.2022.3196835},
	abstract = {��由于电动汽车（EV）的采用率不断提高，迫切需要部署必要的充电站基础设施，以及管理交通流量和拥堵的路线策略。本研究针对具有关于其位置、到达时间和充电时间的随机充电请求的一般EV充电系统的位置路由问题（LRP）。目标是开发电动汽车到充电站的有效路线策略，以及确定最佳充电站位置，以最小化需求的平均响应时间。在对每个充电站的平均等待时间（例如，系统在轻交通或重交通状态下运行）的一些规则性假设下，我们表明优化问题可以被表述为具有大小约束的基于分区的聚类问题。问题公式的这种放松使我们能够开发一种新的数据驱动方法来解决充电站LRP，而无需针对EV的充电请求以及充电站的排队行为建立详细的随机模型。开发了一种算法以及两种大小调整策略，以解决所获得的聚类问题，并在西雅图的城市区域进行了说明，该区域具有各种类型的距离、车辆速度、充电请求位置的分布以及到达之间的时间密度。Due to increasing rates of adoption of electric vehicles (EVs), there is a strong need to deploy the necessary charging station infrastructure, together with routing strategies to manage traffic flow and congestion. This study addresses the location-routing problem (LRP) for a general EV charging system with stochastic charging requests regarding their locations, arrival times and charging times. The objective is to develop an efficient routing strategy of EVs to charging stations, as well as to determine the optimal charging station locations so as to minimize the demand’s mean response time. Under some regularity assumptions on the mean waiting time at each charging station (e.g. system operates in a light or heavy traffic regime), we show that the optimization problem can be formulated as a partition-based clustering problem with size constraints. This relaxation of the problem formulation enables us to develop a novel data-driven approach for solving the charging station LRP, without requiring detailed stochastic models for the EV’s charging requests, as well as the queueing behavior of the charging stations. An algorithm along with two size adjustment strategies are developed to solve the obtained clustering problem and illustrated on urban areas of Seattle with various types of distance, vehicle speeds, distributions for charging request locations, and inter-arrival time densities.},
	number = {12},
	urldate = {2023-03-07},
	journal = {IEEE Transactions on Intelligent Transportation Systems},
	author = {Hung, Ying-Chao and Michailidis, George},
	month = dec,
	year = {2022},
	keywords = {/unread},
	pages = {23858--23868},
}

@article{sadeghianpourhamami_definition_2020,
	title = {Definition and {Evaluation} of {Model}-{Free} {Coordination} of {Electrical} {Vehicle} {Charging} {With} {Reinforcement} {Learning}},
	volume = {11},
	issn = {1949-3053, 1949-3061},
	url = {https://ieeexplore.ieee.org/document/8727484/},
	doi = {10.1109/TSG.2019.2920320},
	abstract = {��需求响应（DR）对于管理日益增长的电动汽车（EV）部署的充电负载变得至关重要。初始DR研究主要采用模型预测控制，但对于EV场景（例如，客户行为），模型在很大程度上是不确定的。基于强化学习（RL）的无模型方法是一种有吸引力的选择。我们在RL框架中提出了一种新的马尔可夫决策过程（MDP）公式，以联合协调一组充电站。现有技术的算法要么专注于单个EV，要么在多个步骤中控制EV的集合（例如，1）做出集合负载决策，2）将集合决策转化为单个EV）。相反，我们的RL方法一次联合控制整个电动汽车。我们提出了一种新的MDP公式，该公式具有独立于充电站数量的可扩展状态表示。使用批量RL算法，拟合Q-迭代，我们学习了最优充电策略。通过使用真实世界数据的模拟，我们：1）区分RL策略训练中的设置（例如，训练数据覆盖的时间跨度）；2） 将其性能与oracle全知基准进行比较（提供性能上限）；3） 分析全年的业绩波动；以及4）证明对更大组充电站的通用能力。Demand response (DR) becomes critical to manage the charging load of a growing electric vehicle (EV) deployment. Initial DR studies mainly adopt model predictive control, but models are largely uncertain for the EV scenario (e.g., customer behavior). Model-free approaches, based on reinforcement learning (RL), are an attractive alternative. We propose a new Markov decision process (MDP) formulation in the RL framework, to jointly coordinate a set of charging stations. State-of-the-art algorithms either focus on a single EV, or control an aggregate of EVs in multiple steps (e.g., 1) make aggregate load decisions and 2) translate the aggregate decision to individual EVs). In contrast, our RL approach jointly controls the whole set of EVs at once. We contribute a new MDP formulation with a scalable state representation independent of the number of charging stations. Using a batch RL algorithm, fitted Q-iteration, we learn an optimal charging policy. With simulations using real-world data, we: 1) differentiate settings in training the RL policy (e.g., the time span covered by training data); 2) compare its performance to an oracle all-knowing benchmark (providing an upper performance bound); 3) analyze performance fluctuations throughout a full year; and 4) demonstrate generalization capacity to larger sets of charging stations.},
	number = {1},
	urldate = {2023-03-07},
	journal = {IEEE Transactions on Smart Grid},
	author = {Sadeghianpourhamami, Nasrin and Deleu, Johannes and Develder, Chris},
	month = jan,
	year = {2020},
	keywords = {/unread},
	pages = {203--214},
	file = {Sadeghianpourhamami et al_2020_Definition and Evaluation of Model-Free Coordination of Electrical Vehicle.pdf:D\:\\ZoteroData\\storage\\Q2HPS9RY\\Sadeghianpourhamami et al_2020_Definition and Evaluation of Model-Free Coordination of Electrical Vehicle.pdf:application/pdf},
}

@article{duan_planning_2022,
	title = {Planning {Strategy} for an {Electric} {Vehicle} {Fast} {Charging} {Service} {Provider} in a {Competitive} {Environment}},
	volume = {8},
	issn = {2332-7782, 2372-2088},
	url = {https://ieeexplore.ieee.org/document/9715093/},
	doi = {10.1109/TTE.2022.3152387},
	abstract = {��快速充电是电动汽车（EV）的一种很有前途的能量补充方法，可将补充时间大大缩短至与行驶和排队时间相当的水平，因此将极大地影响EV用户的充电地点的决定。多个充电服务提供商（CSP）参与电动汽车快速充电设施的投资，不可避免地带来了彼此之间的竞争，这影响了投资者的规划决策。本文研究了竞争环境下电动汽车快速充电基础设施的规划策略，考虑了排队对电动汽车用户的影响。提出了一种双环迭代方法，外环寻求CSP规划方案的平衡点。在内环中建立了与排队理论和自适应路线选择算法相结合的用户均衡交通分配模型，以优化CSP的规划方案，以及车站级线性规划模型，以获得考虑使用时间价格和投资约束的现场储能系统（ESS）的最优规模和运行调度。案例研究验证了所提出方法的有效性，并表明忽略竞争对手的响应将导致对CSP吸引的充电需求及其总收益的高估。Fast charging is a promising method of energy refueling for electric vehicles (EVs), significantly shortening the refueling time to a level comparable with traveling and queuing times, which consequently will highly influence the decision of charging places for EV users. Multiple charging service providers (CSPs) are participating in the investment of EV fast charging facilities, inevitably bringing competition among each other, which affects the planning decision of investors. This article studies the planning strategy of EV fast charging infrastructure in a competitive environment, taking the queuing impacts on EV users into consideration. A double-loop iterative methodology is proposed, and the outer loop seeks the equilibrium point of the CSP’s planning schemes. A user equilibrium traffic assignment model integrated with queuing theory and an adaptive route selection algorithm are built in the inner loop to optimize the planning scheme of a CSP, along with a station-level linear programming model to obtain the optimal sizes and operation scheduling of on-site energy storage systems (ESSs) considering time-of-use prices and investment constraints. Case studies verify the effectiveness of the proposed method and reveal that ignoring the competitors’ response will cause overestimation of the CSP’s attracted charging demands and its total benefits.},
	number = {3},
	urldate = {2023-03-07},
	journal = {IEEE Transactions on Transportation Electrification},
	author = {Duan, Xiaoyu and Hu, Zechun and Song, Yonghua and Strunz, Kai and Cui, Yan and Liu, Likai},
	month = sep,
	year = {2022},
	keywords = {/unread},
	pages = {3056--3067},
}

@article{sohet_coupled_2021,
	title = {Coupled {Charging}-and-{Driving} {Incentives} {Design} for {Electric} {Vehicles} in {Urban} {Networks}},
	volume = {22},
	issn = {1524-9050, 1558-0016},
	url = {https://ieeexplore.ieee.org/document/9090977/},
	doi = {10.1109/TITS.2020.2991352},
	abstract = {��电动汽车（EV）在行驶（例如，减少噪音和污染）和充电时都会影响城市网络。对于电网而言，电动汽车充电的灵活性使其成为“需求响应”机制的重要参与者。因此，有必要设计激励机制以促进客户参与。采用拥堵博弈方法来评估具有多类车辆（电动汽车和汽油车）的此类电动交通系统的性能。同时考虑了时间和能源运营成本。后者是不可分割的，因为它取决于所有电动汽车的全球充电需求，由集中聚合器根据充电地点的非柔性消耗及时调度。因此，驾驶和充电决策是耦合的。对Beckmann方法的一种改进证明了所考虑的不可分拥塞博弈中Wardrop均衡（WE）的存在性；当充电单价是全球充电需求的增加函数时，这个WE是唯一的。给出了一个关于非柔性负载的条件，以保证该函数的单调性。该条件在法国和美国德克萨斯州的实际消耗数据上进行了测试。最优通行费用于控制该电力运输系统，然后进行计算，以便在简单的网络拓扑上最小化环境成本。Electric Vehicles (EV) impact urban networks both when driving (e.g., noise and pollution reduction) and charging. For the electrical grid, the flexibility of EV charging makes it a significant actor in “Demand Response” mechanisms. Therefore, there is a need to design incentive mechanisms to foster customer engagement. A congestion game approach is adopted to evaluate the performance of such electrical transportation system with multiple classes of vehicles: EV and Gasoline Vehicles. Both temporal and energy operating costs are considered. The latter is nonseparable as it depends on the global charging need of all EV, which is scheduled in time by a centralized aggregator in function of nonflexible consumption at charging location. Thus, driving and charging decisions are coupled. An adaptation of Beckmann’s method proves the existence of a Wardrop Equilibrium (WE) in the considered nonseparable congestion game; this WE is unique when the charging unit price is an increasing function of the global charging need. A condition on the nonflexible load is given to guarantee the monotonicity of this function. This condition is tested on real consumption data in France and in Texas, USA. Optimal tolls are used to control this electrical transportation system and then computed in order to minimize an environmental cost on a simple network topology.},
	number = {10},
	urldate = {2023-03-07},
	journal = {IEEE Transactions on Intelligent Transportation Systems},
	author = {Sohet, Benoit and Hayel, Yezekael and Beaude, Olivier and Jeandin, Alban},
	month = oct,
	year = {2021},
	keywords = {/unread},
	pages = {6342--6352},
	file = {Sohet et al_2021_Coupled Charging-and-Driving Incentives Design for Electric Vehicles in Urban.pdf:D\:\\ZoteroData\\storage\\6SJD8MF6\\Sohet et al_2021_Coupled Charging-and-Driving Incentives Design for Electric Vehicles in Urban.pdf:application/pdf},
}

@article{lin_minimizing_2022,
	title = {Minimizing {Cost}-{Plus}-{Dissatisfaction} in {Online} {EV} {Charging} {Under} {Real}-{Time} {Pricing}},
	volume = {23},
	issn = {1524-9050, 1558-0016},
	url = {https://ieeexplore.ieee.org/document/9552612/},
	doi = {10.1109/TITS.2021.3114537},
	abstract = {We consider an increasingly popular demand-response scenario where a home user schedules the flexible electric vehicle (EV) charging load in response to real-time electricity prices. The objective is to minimize the total charging cost with user dissatisfaction taken into account. We focus on the online setting where neither accurate prediction nor distribution of future real-time prices is available to the user when making irrevocable charging decisions in each time slot. The emphasis on considering user dissatisfaction and achieving optimal competitive ratio differentiates our work from existing ones and makes our study uniquely challenging. Our key contribution is two simple online algorithms with the optimal competitive ratio among all deterministic algorithms. The optimal competitive ratio is upper-bounded by min\{α/pmin−−−−−−√,pmax/pmin\} and the bound is asymptotically tight with respect to α , where pmax and pmin are the upper and lower bounds of real-time prices and α≥pmin captures the consideration of user dissatisfaction. The bounds with respect to small and large values of α suggest the fundamental difference of the problems with and without considering user dissatisfaction. We also extend the algorithms to take minimum charging requirement and short-term prediction into account. Simulation results based on real-world traces corroborate our theoretical findings and show that the empirical performance of our algorithms can be substantially better than the theoretical worst-case guarantees. Our algorithms also achieve notable performance gains under diverse settings as compared to conceivable alternatives.},
	number = {8},
	urldate = {2023-03-07},
	journal = {IEEE Transactions on Intelligent Transportation Systems},
	author = {Lin, Qiulin and Yi, Hanling and Chen, Minghua},
	month = aug,
	year = {2022},
	keywords = {/unread},
	pages = {12464--12479},
	file = {Lin et al_2022_Minimizing Cost-Plus-Dissatisfaction in Online EV Charging Under Real-Time.pdf:D\:\\ZoteroData\\storage\\NTRC95H2\\Lin et al_2022_Minimizing Cost-Plus-Dissatisfaction in Online EV Charging Under Real-Time.pdf:application/pdf},
}

@article{wang_coordinated_2023,
	title = {Coordinated {Electric} {Vehicle} {Active} and {Reactive} {Power} {Control} for {Active} {Distribution} {Networks}},
	volume = {19},
	issn = {1551-3203, 1941-0050},
	url = {https://ieeexplore.ieee.org/document/9762523/},
	doi = {10.1109/TII.2022.3169975},
	abstract = {��在电力系统中部署可再生能源可能会引起严重的电压不稳定性。电动汽车（EV）由于其移动性和灵活性特点，可以提供各种辅助服务，包括有功和无功功率。然而，在这种情况下，电动汽车的分布式控制是一个复杂的决策问题，具有巨大的动态性和不确定性。大多数现有文献采用基于模型的方法来制定有功和无功功率控制问题，这需要完整的模型并且耗时。本文提出了一种具有深度确定性策略梯度（DDPG）方法和参数共享框架的多智能体强化学习算法，以解决电动汽车针对需求侧响应和电压调节的协调有功和无功功率控制问题。所提出的算法可以通过位置边际价格进一步增强学习稳定性和可扩展性，并具有隐私持久性。基于改进的IEEE 15总线网络的仿真结果被开发，以验证其在提供系统充电和电压调节服务方面的有效性。所提出的位置边际价格（LMP）PSDDPG算法被评估为比基准DDPG、TD3和LMP-DDPG分别实现38\%、16\%和25\%的加速，以及1.58、0.69和0.27倍的奖励。The deployment of renewable energy in power systems may raise serious voltage instabilities. Electric vehicles (EVs), owing to their mobility and flexibility characteristics, can provide various ancillary services including active and reactive power. However, the distributed control of EVs under such scenarios is a complex decision-making problem with enormous dynamics and uncertainties. Most existing literature employs model-based approaches to formulate active and reactive power control problems, which require full models and are time-consuming. This article proposes a multiagent reinforcement learning algorithm featuring a deep deterministic policy gradient (DDPG) method and a parameter sharing framework to solve the EVs’ coordinated active and reactive power control problem toward both demand-side response and voltage regulations. The proposed algorithm can further enhance the learning stability and scalability with privacy perseverance via the location marginal prices. Simulation results based on a modified IEEE 15-bus network are developed to validate its effectiveness in providing system charging and voltage regulation services. The proposed location marginal price (LMP) PSDDPG algorithm is evaluated to achieve 38\%, 16\%, and 25\% speedup, and 1.58, 0.69, and 0.27 times higher reward over the benchmarks DDPG, TD3, and LMP-DDPG, respectively.},
	number = {2},
	urldate = {2023-03-07},
	journal = {IEEE Transactions on Industrial Informatics},
	author = {Wang, Yi and Qiu, Dawei and Strbac, Goran and Gao, Zhiwei},
	month = feb,
	year = {2023},
	keywords = {/unread},
	pages = {1611--1622},
	file = {Wang et al_2023_Coordinated Electric Vehicle Active and Reactive Power Control for Active.pdf:D\:\\ZoteroData\\storage\\RA6M6YVH\\Wang et al_2023_Coordinated Electric Vehicle Active and Reactive Power Control for Active.pdf:application/pdf},
}

