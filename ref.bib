@article{yan_optimized_2019,
	title = {Optimized {Operational} {Cost} {Reduction} for an {EV} {Charging} {Station} {Integrated} {With} {Battery} {Energy} {Storage} and {PV} {Generation}},
	volume = {10},
	issn = {1949-3053, 1949-3061},
	url = {https://ieeexplore.ieee.org/document/8242670/},
	doi = {10.1109/TSG.2017.2788440},
	abstract = {��本文提出了一种电动汽车（EV）双向充电站的四阶段智能优化和控制算法，该充电站配备光伏发电和固定电池储能，并与商业建筑集成。所提出的算法旨在考虑潜在的不确定性，最大限度地降低客户满意度所涉及的运营成本，同时通过调整电动汽车移动/本地电池存储、电网供应和延迟负载的最佳计划充电/放电来平衡实时供需。机会约束优化目标分为以下几个阶段：1）第一阶段，日前能源管理计划的优化；2） 第二阶段，多层电动汽车充电价格更新和放电参与奖金优化；3） 第三阶段，优化提前一小时的能源管理计划；和4）阶段IV，实时控制。这种算法为不可预测的条件提供了更大的弹性，为电动汽车用户提供了更多的参与激励，并更好地协调包括建筑负荷在内的集成系统，以可靠地为客户服务，同时降低成本。实施了案例研究，并根据算法的每个设计特征的使用和效益进行了比较分析。结果表明，所提出的算法可以降低操作成本，同时对不确定性提供更高的容忍度。A four-stage intelligent optimization and control algorithm for an electric vehicle (EV) bidirectional charging station equipped with photovoltaic generation and fixed battery energy storage and integrated with a commercial building is proposed in this paper. The proposed algorithm aims at maximally reducing the customer satisfaction-involved operational cost considering the potential uncertainties, while balancing the real-time supply and demand by adjusting the optimally scheduled charging/discharging of EV mobile/local battery storage, grid supply, and deferrable load. The chanceconstrained optimization objective has been stated in stages: 1) stage I, optimization of day-ahead energy management schedules; 2) stage II, multitiered EV charging price update and optimization of discharging participation bonus; 3) stage III, optimization of hour-ahead energy management schedules; and 4) stage IV, real-time control. Such algorithm provides more resilience for unpredictable conditions, provides more incentives for EV users to participate, and better coordinates the integrated system including the building load to reliably serve the customers while lessening cost. Case studies are implemented and the comparison analysis is performed in terms of the use and benefit of each design feature of the algorithm. The results indicate that the proposed algorithm can reduce the operational cost and at the same time provide higher tolerability toward uncertainties.},
	number = {2},
	urldate = {2022-10-11},
	journal = {IEEE Transactions on Smart Grid},
	author = {Yan, Qin and Zhang, Bei and Kezunovic, Mladen},
	month = mar,
	year = {2019},
	pages = {2096--2106},
	file = {Yan et al_2019_Optimized Operational Cost Reduction for an EV Charging Station Integrated With.pdf:D\:\\ZoteroData\\storage\\V4VWZ8F2\\Yan et al_2019_Optimized Operational Cost Reduction for an EV Charging Station Integrated With.pdf:application/pdf},
}

@article{wan_model-free_2019,
	title = {Model-{Free} {Real}-{Time} {EV} {Charging} {Scheduling} {Based} on {Deep} {Reinforcement} {Learning}},
	volume = {10},
	issn = {1949-3053, 1949-3061},
	shorttitle = {��基于深度强化学习的无模型实时电动汽车充电调度},
	url = {https://ieeexplore.ieee.org/document/8521585/},
	doi = {10.1109/TSG.2018.2879572},
	abstract = {��在电动汽车( Electric Vehicle，EV )技术进步的推动下，电动汽车已成为智能电网经济的重要组成部分。当电动汽车参与具有实时定价信号的需求响应计划时，充分利用这些定价信号可以大大降低充电成本。然而，由于交通状况、用户通勤行为以及效用的定价过程中存在随机性，确定最优的充电策略具有挑战性。传统的基于模型的方法需要对调度过程的不确定性和优化进行预测。本文将该调度问题建模为转移概率未知的马尔科夫决策过程( Markov Decision Process，MDP )。提出了一种基于深度强化学习的无模型方法来确定该问题的最优策略。所提方法能够自适应地学习转移概率，且不需要任何系统模型信息。体系结构Driven by the recent advances in electric vehicle (EV) technologies, EVs have become important for smart grid economy. When EVs participate in demand response program which has real-time pricing signals, the charging cost can be greatly reduced by taking full advantage of these pricing signals. However, it is challenging to determine an optimal charging strategy due to the existence of randomness in traffic conditions, user’s commuting behavior, and the pricing process of the utility. Conventional model-based approaches require a model of forecast on the uncertainty and optimization for the scheduling process. In this paper, we formulate this scheduling problem as a Markov Decision Process (MDP) with unknown transition probability. A model-free approach based on deep reinforcement learning is proposed to determine the optimal strategy for this problem. The proposed approach can adaptively learn the transition probability and does not require any system model information. The architecture of the proposed approach contains two networks: a representation network to extract discriminative features from the electricity prices and a Q network to approximate the optimal action-value function. Numerous experimental results demonstrate the effectiveness of the proposed approach.},
	number = {5},
	urldate = {2022-11-20},
	journal = {IEEE Transactions on Smart Grid},
	author = {Wan, Zhiqiang and Li, Hepeng and He, Haibo and Prokhorov, Danil},
	month = sep,
	year = {2019},
	pages = {5246--5257},
	file = {Wan et al_2019_Model-Free Real-Time EV Charging Scheduling Based on Deep Reinforcement Learning.pdf:D\:\\ZoteroData\\storage\\FUUSBQBJ\\Wan et al_2019_Model-Free Real-Time EV Charging Scheduling Based on Deep Reinforcement Learning.pdf:application/pdf},
}

@article{ali_optimal_2022,
	title = {Optimal operation of static energy storage in fast-charging stations considering the trade-off between resilience and peak shaving},
	volume = {53},
	issn = {2352152X},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S2352152X22011963},
	doi = {10.1016/j.est.2022.105197},
	abstract = {��电动汽车（EV）渗透率的提高给电力网络带来了一些挑战，例如不确定的峰值负载和停电期间的弹性问题。通过在充电站中使用本地静态电池能量存储系统（BESS），可以实现弹性和调峰功能。然而，弹性和峰值是矛盾的，即增加一个会使另一个恶化。因此，在本研究中，提出了一种弹性和调峰权衡方案，以优化利用静态BESS。首先，制定了一个弹性窗口，以确定BESS中存储的能量量，以便在任何意外情况下电动汽车的弹性。在一天的高峰时段，更重要的是调峰，而在非高峰时段，恢复力被优先考虑。然后，开发了一种优化算法，以最小化系统成本，同时保持弹性并最大化调峰。使用所提出的窗口方法，通过归一化方法确定了最佳窗口大小。通过考虑弹性和调峰的冲突性质，已经进行了模拟，以显示所提出的方案的有效性。借助所提出的策略，实现了额外3.9\%的调峰和3.41\%的运营成本降低。此外，通过考虑可能影响弹性窗口最佳大小的不同因素（市场价格、电动汽车车队规模和BESS规模）进行了敏感性分析。Enhanced penetration of electric vehicles (EVs) poses several challenges to the power network, such as uncertain peak loads and resilience issues during outages. Both resilience and peak shaving functions can be achieved by using a local static battery energy storage system (BESS) in the charging stations. However, resilience and peakshaving are contradictory, i.e. increasing one will deteriorate the other. Therefore, in this study, a resilience and peak shaving trade-off scheme is proposed to optimally utilize the static BESS. Firstly, a resilience window is formulated to determine the amount of energy to be stored in the BESS for the resilienceof EVs in the case of any contingency. During peak hours of the day, more importance is given to peak-shaving, whereas in the off-peak hours, resilience is prioritized. Then, an optimization algorithm is developed to minimize the cost of the system while maintaining resilience and maximizing peak shaving. Using the proposed window method, an optimal window size has been determined via a normalization approach. Simulations have been carried out to show the effectiveness of the proposed scheme by considering the conflicting nature of resilience and peak shaving. With the help of the proposed strategy additional 3.9 \% of peak shaving and 3.41 \% reduction operational costs is achieved. Moreover, sensitivity analysis has been carried out by considering different factors (market price, size of EV fleet, and BESS size) that can affect the optimal size of the resilience window.},
	language = {en},
	urldate = {2023-03-07},
	journal = {Journal of Energy Storage},
	author = {Ali, Asfand Yar and Hussain, Akhtar and Baek, Ju-Won and Kim, Hak-Man},
	month = sep,
	year = {2022},
	keywords = {/unread},
	pages = {105197},
	file = {Ali et al_2022_Optimal operation of static energy storage in fast-charging stations.pdf:D\:\\ZoteroData\\storage\\VSZC83FS\\Ali et al_2022_Optimal operation of static energy storage in fast-charging stations.pdf:application/pdf},
}

@article{elma_dynamic_2020,
	title = {A dynamic charging strategy with hybrid fast charging station for electric vehicles},
	volume = {202},
	issn = {03605442},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0360544220307878},
	doi = {10.1016/j.energy.2020.117680},
	abstract = {��随着技术进步和环境问题，电动汽车（EV）的普及率一直在上升。充电时间和充电需求是电动汽车适应的重要挑战。为了应对这些挑战，本研究提出了一种具有动态能量管理系统的直流快速充电技术。然而，DC快速充电器需要高功率需求周期以减少充电时间。这反过来会对电网造成负面影响，如稳定性、弹性和效率问题。本研究的目的是评估混合直流快速充电站，以减少充电期间的峰值需求。所提出的能量管理算法与动态数据使用一起在这样的系统操作上提供了更可靠的结果。通过所提出的控制算法，电网的峰值需求大幅减少了45\%，并且由于更可控的充电/放电协调，电池寿命得以延长。The popularity of electric vehicles (EV) have been on the rise with technological advancements and environmental concerns. The charging time and charging demand are important challenges for EV adaptation. In order to address these challenges, a DC fast charging technology with a dynamic energy management system is proposed in this study. However, DC fast chargers require high power demand periods to reduce the charging time. This, in turn, will cause negative effects on the grid such as stability, resilience, and efficiency problems. The purpose of the study is to evaluate a hybrid DC fast charging station with the aim of reducing peak demand during charging periods. The proposed energy management algorithm together with the dynamic data use provides more reliable results on such systems operations. With the proposed control algorithm, both peak demand from the grid is substantially reduced by 45\% and the battery life span is extended thanks to more controlled charge/discharge coordination.},
	language = {en},
	urldate = {2023-03-07},
	journal = {Energy},
	author = {Elma, Onur},
	month = jul,
	year = {2020},
	keywords = {/unread},
	pages = {117680},
}

